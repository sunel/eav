{
    "name": "sunel/eav",
    "description": "Eav For Artisan.",
    "keywords": ["eav", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0",
        "league/csv":"^9.0",
        "sunel/json-api-resources":">=0.0.1"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "fzaninotto/faker": "~1.4"
    },
    "authors": [
        {
            "name": "Sunel TR",
            "email": "sunelbe@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eav\\TestCase\\" : "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eav\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}
